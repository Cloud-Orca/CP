    <sharingCriteriaRules>
        <fullName>CF_Confidentiality</fullName>
        <accessLevel>Edit</accessLevel>
        <label>CF Confidentiality</label>
        <sharedTo>
            <group>Corporate_Finance_Group</group>
        </sharedTo>
        <criteriaItems>
            <field>CP_Confidential_Area__c</field>
            <operation>equals</operation>
            <value>Corporate Finance</value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
